[color]
  diff = auto
  status = auto
  branch = auto
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[user]
  email = nlacasse@borderstylo.com
  name = Nicolas LaCasse
[branch]
  autosetupmerge = true
[core]
  excludesfile = /home/nlacasse/.gitignore
  pager = cat
  whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  lg = log -p
[push]
	default = simple
